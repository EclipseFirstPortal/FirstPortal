aimfovcircle_outer.Color = Color3.new()
aimfovcircle_outer.Position = Vector2.new(Camera.ViewportSize.X / 2, Camera.ViewportSize.Y / 2)
aimfovcircle_outer.NumSides = aimfovcircle_numsides

