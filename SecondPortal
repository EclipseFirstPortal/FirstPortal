aimfovcircle_inner.Color = Color3.new()
aimfovcircle_inner.Position = Vector2.new(Camera.ViewportSize.X / 2, Camera.ViewportSize.Y / 2)
aimfovcircle_inner.NumSides = aimfovcircle_numsides

