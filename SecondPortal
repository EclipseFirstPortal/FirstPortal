    Name = "Remove Mines",
    Flag = "RemoveMines",
    Callback = function(value)
        removeMines = value
        showToggleNotification("Remove Mines", value)
        playToggleSound(value)
    end
})



task.spawn(function()
    while true do
        if removeMines then
            for _, v in ipairs(workspace:GetDescendants()) do
                if table.find(minesToDelete, v.Name) and v:IsA("Model") then
                    v:Destroy()
                end
            end
        end
        wait(1)
    end
end)
